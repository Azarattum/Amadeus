{
  "name": "ui",
  "type": "module",
  "exports": {
    "./*": "./lib/*/index.ts",
    "./tailwind": "./lib/tailwind.pcss",
    "./theme": "./lib/theme.cjs"
  },
  "typesVersions": {
    "*": {
      "*": [
        "./lib/*/index.ts"
      ]
    }
  },
  "scripts": {
    "dev": "vite dev -c ../../vite.config.js --mode svelte --port 5174",
    "prepare": "svelte-kit sync"
  },
  "devDependencies": {
    "svelte": "^3.44.0",
    "svelte-preprocess": "^4.10.6"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "vite"
      ]
    }
  }
}
