{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "importsNotUsedAsValues": "error",
    "preserveValueImports": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "isolatedModules": true,
    "module": "esnext",
    "target": "esnext",
    "lib": ["ESNext"],
    "types": ["node"],
    "strict": true
  }
}
