{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "dev": {
      "cache": false
    },
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["../../build/**"]
    },
    "preview": {
      "dependsOn": ["build"],
      "outputs": [],
      "cache": false
    },
    "test": {
      "outputs": []
    },
    "coverage": {
      "outputs": ["coverage/**"]
    }
  }
}
