{
  "cSpell.words": [
    "autoprefixer",
    "cssnano",
    "econd",
    "esbuild",
    "HTMLI",
    "iframe",
    "lcov",
    "pcss",
    "pnpm",
    "postcss",
    "preprocess",
    "rescape",
    "rimraf",
    "rollup",
    "superstruct",
    "sveltekit",
    "tailwindcss",
    "ttypescript",
    "unprefix",
    "unprefixes",
    "vitest"
  ],
  "eslint.validate": ["javascript", "typescript", "svelte"],
  "prettier.documentSelectors": ["**/*.svelte"],
  "files.exclude": {
    "**/node_modules": true,
    "**/.svelte-kit": true,
    "**/coverage": true,
    "**/.vscode": true,
    "**/.turbo": true,
    "*pnpm*": true
  },
  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  }
}
